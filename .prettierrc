{
    "useTabs": false,
    "endOfLine": "lf",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "",
        "^#",
        "",
        "^[./]"
    ],
    "importOrderParserPlugins": [
        "typescript",
        "decorators-legacy"
    ],
    "plugins": [
        "@ianvs/prettier-plugin-sort-imports"
    ]
}